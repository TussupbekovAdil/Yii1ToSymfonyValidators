{
    "name": "wooppay/yii-to-symfony-validators-bundle",
    "description": "Yii to Symfony Validators Converter Bundle",
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*",
        "symfony/translation": "4.4",
        "symfony/validator": "^4.4",
        "doctrine/annotations": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "wooppay\\YiiToSymfonyValidatorsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "wooppay\\YiiToSymfonyValidatorsBundle\\Tests\\": "tests/"
        }
    }
}
